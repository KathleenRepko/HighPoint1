define(function (require) {
  /* eslint no-var:0 */
  var ALModel = require('al/al_model');

  // For now UserModel may be treated like a special singleton.
  var UserModel = ALModel.extend({
    isAuthorized: function () {
      return (this.has('userId') || this.has('id'));
    }
  });

  return UserModel;
});
